{
  "manifest_version": 2,
  "name": "dblclick-image-download",
  "version": "1.3",
  "description": "Downloads images when you double-click on them",
  "icons": {
    "128": "icons/icon-128.png",
    "64": "icons/icon-64.png",
    "48": "icons/icon-48.png"
  },
  "background": {
    "scripts": [
      "background/background.js",
      "background/onFocus.js"
    ]
  },
  "permissions": [
    "downloads",
    "tabs",
    "<all_urls>"
  ]
}
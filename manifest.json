{
  "manifest_version": 2,
  "name": "dblclick-image-download",
  "version": "2.0",
  "description": "Downloads an image on dblclick",
  "icons": {
    "48": "icons/avatar-48.png"
  },
  "background": {
    "scripts": [
      "background/background.js",
      "background/onFocus.js"
    ]
  },
  "permissions": [
    "downloads",
    "tabs",
    "<all_urls>"
  ]
}